---
apiVersion: apps/v1
kind: Deployment
metadata:
  name: mydep
  namespace: test
  labels:
    app: myapp
spec:
  replicas: 1
  selector:
    matchLabels:
      app: myapp
  template:
    metadata:
      labels:
        app: myapp
    spec:
      containers:
      - name: myapp
        image: crccheck/hello-world
        ports:
        - containerPort: 80
---
